{
  "name": "@ziuchen/bytemd-plugin-highlight-theme",
  "private": false,
  "version": "1.0.8",
  "description": "ByteMD plugin to support custom code highlight theme",
  "license": "MIT",
  "author": "ZiuChen",
  "type": "module",
  "scripts": {
    "build": "rolldown -c",
    "dev": "rolldown --watch -c"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ZiuChen/bytemd-plugin.git",
    "directory": "packages/bytemd-plugin-highlight-theme"
  },
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "main": "./dist/index.js",
  "unpkg": "./dist/index.umd.js",
  "jsdelivr": "./dist/index.umd.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./locales/*": "./locales/*",
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist",
    "locales"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@bytemd/plugin-frontmatter": "^1.20.2",
    "@bytemd/plugin-highlight": "^1.20.2"
  },
  "peerDependencies": {
    "bytemd": "^1.20.2"
  }
}
